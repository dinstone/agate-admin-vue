    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persist": "^1.0.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "url": "^0.11.3",
    "vue": "3.3.4",
    "vue-i18n": "9.4.1",
    "vue-json-pretty": "^2.2.4",
    "vue-router": "^4.2.5",
    "vue-types": "^5.1.1"
